{
  "title": "workflow1",
  "description": "test-workflow",
  "variables": [
    "order"
  ],
  "sequence": [
    {
      "title": "MarketWS",
      "RAML-File": "G:/GIT Repositorys/RESTFlow/src/main/resources/SampleRessources/RAML-Files/Market.raml",
      "resource": "/getBestOffer",
      "input": {
        "user_parameter": [
          {
            "name": "partNumber",
            "type": "string"
          },
          {
            "name": "budget",
            "type": "double"
          }
        ]
      },
      "output": [
        {
          "name": "order",
          "type": "MarketWS.Type.Order"
        }
      ],
      "assign": {
        "from": "OUTPUT.order",
        "to": "VARIABLES.order"
      }
    },
    {
      "title": "OrderWS",
      "RAML-File": "G:/GIT Repositorys/RESTFlow/src/main/resources/SampleRessources/RAML-Files/Order.raml",
      "resource": "/placeOrder",
      "input": {
        "parameter": [
          "VARIABLES.order"
        ]
      },
      "output": [
        {
          "name": "confirmation",
          "type": "string"
        }
      ]
    }
  ]
}
