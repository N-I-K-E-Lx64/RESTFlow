{
  "workflow": {
    "title": "test",
    "description": "test",
    "variables": [
      "order",
      "order_confirmation",
      "RESULT"
    ],
    "process": {
      "invoke": [
        {
          "title": "MarketWS",
          "raml": "Market.raml",
          "resource": "/getBestOffer",
          "input": {
            "user-parameter": [
              {
                "name": "partNumber",
                "type": "String"
              },
              {
                "name": "budget",
                "type": "double"
              }
            ]
          },
          "validator": "MarketWS.TYPE.Order",
          "assignTo": "VARIABLES.order"
        },
        {
          "title": "OrderWS",
          "raml": "Order.raml",
          "resource": "/placeOrder",
          "input": {
            "variables": {
              "variable": "order"
            }
          },
          "assignTo": "VARIABLES.order_confirmation"
        }
      ],
      "switch": {
        "condition": {
          "operator": "CONTAINS",
          "value1": "VARIABLES.order_confirmation",
          "value2": {
            "type": "String",
            "name": "checkResult",
            "value": "Success"
          }
        },
        "case": {
          "assign": {
            "source": {
              "parameter": {
                "type": "String",
                "name": "SuccessResult",
                "value": "Success"
              }
            },
            "target": "VARIABLES.RESULT"
          }
        },
        "otherwise": {
          "assign": {
            "source": {
              "parameter": {
                "type": "String",
                "name": "ErrorResult",
                "value": "Error"
              }
            },
            "target": "VARIABLES.RESULT"
          }
        }
      }
    }
  }
}
